{
    "compilerOptions": {
        "module": "CommonJS",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "inlineSourceMap": true,
        "esModuleInterop": true,
        "lib": ["es2015"],
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "downlevelIteration": true
    },
    "include": ["src/api/**/*"],
    "exclude": ["**/*.test.ts", "**/*.test.tsx"]
}
